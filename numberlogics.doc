import java.util.*;
class HelloWorld {
    
    public static int prime(int n)
    {
        boolean flag=true;
        if(n==1||n==0)
        return 0;
        if(n==2)
        return 1;
        if(n%2==0)
        return 0;
        for(int i=3;i<n/2;i++)
        {
            if(n%i==0)
            {
                flag=false;
                break;
            }
        }
        if(flag)
        return 1;
    return 0;
        
    }
    public static void main(String[] args) {
       Scanner sc =new Scanner(System.in);
       int n=sc.nextInt();
       int res=prime(n);
       if(res==1)
       System.out.println("PRIME");
       else
       System.out.println("NOT A PRIME");
    }
}


