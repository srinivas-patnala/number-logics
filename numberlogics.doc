import java.util.*;
class HelloWorld {
    public static int reverse(int n)
    {
        int rev=0;
        if(n<0)
        {
            n=-n;
            while(n!=0)
            {
                rev=rev*10+n%10;
                n=n/10;
             }
        return -rev;
        }
        while(n!=0)
        {
        rev=rev*10+n%10;
        n=n/10;
       }
        return rev;
    }
    
    public static void main(String[] args) {
       Scanner sc =new Scanner(System.in);
       int n=sc.nextInt();
       int res=reverse(n);
       System.out.println(res);
    }
}


