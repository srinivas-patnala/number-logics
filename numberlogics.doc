import java.util.*;
class HelloWorld {
    public static int sum( int n)
    {
        int res=0;
        for(int i=1;i<=n;i++)
        {
            res=res+i;
        }
        return res;
    }
    
    public static void main(String[] args) {
       Scanner sc =new Scanner(System.in);
       int n=sc.nextInt();
       int res=sum(n);
       System.out.println(res);
    }
}
