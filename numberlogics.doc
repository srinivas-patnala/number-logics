import java.util.*;
class HelloWorld {
    public static int[] factors(int n)
{
    int a[]=new int[n];
    int c=0;
    for(int i=1;i<=n;i++)
    {
        
        if(n%i==0)
        {
            a[c]=i;
            c++;
        }
    }
    int res[]=new int[c];
    for(int i=0;i<c;i++)
    res[i]=a[i];
    return res;
}
public static int abudant(int n)
{
    int res[]=factors(n);
    int sum=0;
    for(int i=0;i<res.length-1;i++)
    {
        sum=sum+res[i];
    }
    return (sum>n)?1:0;
}

    public static void main(String[] args) {
       Scanner sc =new Scanner(System.in);
       int n=sc.nextInt();
       int res=abudant(n);
       if(res==1)
       System.out.println("ABUDANT NUMBER");
       else
       System.out.println("NOT A ABUDANT NUMBER");


    }
}


