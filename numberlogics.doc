import java.util.*;
class HelloWorld {
public static int[] factors(int n)
{
    int a[]=new int[n];
    int c=0;
    for(int i=1;i<=n;i++)
    {
        
        if(n%i==0)
        {
            a[c]=i;
            c++;
        }
    }
    int res[]=new int[c];
    for(int i=0;i<c;i++)
    res[i]=a[i];
    return res;
}
  
    
    public static void main(String[] args) {
       Scanner sc =new Scanner(System.in);
      
       int n=sc.nextInt();
       int res[]=factors(n);
       for(int i:res)
       System.out.print(i+" ");
    }
}
