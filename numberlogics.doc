prime factors*********************


import java.util.*;

class HelloWorld {
    public static int[] factors(int n)
    {
        int[] a = new int[n];
        int c = 0;
        boolean flag;

        for (int i = 1; i <= n; i++)
        {
            if (n % i == 0)
            {
                if (i > 1)
                {
                    flag = true;
                    for (int j = 2; j <= i / 2; j++) 
                    {
                        if (i % j == 0) 
                        {
                            flag = false;
                            break;
                        }
                    }
                } 
                else
                {
                    flag = false;
                }
                if (flag) 
                {
                    a[c] = i;
                    c++;
                }
            }
        }

        int[] res = new int[c];
        for (int k = 0; k < c; k++)
        {
            res[k] = a[k];
        }
        return res;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] res = factors(n);
        for (int i : res) {
            System.out.print(i + " ");
        }
    }
}


