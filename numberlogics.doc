import java.util.*;
class HelloWorld {
    public static int fact( int n)
    {
        int res=1;
        for(int i=n;i>=1;i--)
        {
            res=res*i;
        }
        return res;
    }
    
    
    public static void main(String[] args) {
       Scanner sc =new Scanner(System.in);
       int n=sc.nextInt();
       int res=fact(n);
       System.out.println(res);
    }
}

